<?xml version="1.0" encoding="utf-8" ?>
<ContentPage xmlns="http://xamarin.com/schemas/2014/forms"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="jamesMont.View.ChangeBooking">
    <ContentPage.Content >
        <StackLayout BackgroundColor="#3897f0">
            <Button Text="Change Booking" Clicked="changeBooking" Image="spanner.png"/>
            <Button Text="Cancel Booking" Clicked="deleteBooking" Image="cancel.png"/>
            <Image x:Name="image" HeightRequest="200" WidthRequest="200"/>

        </StackLayout>
    </ContentPage.Content>
</ContentPage>