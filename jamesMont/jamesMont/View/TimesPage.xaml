<?xml version="1.0" encoding="utf-8" ?>
<ContentPage xmlns="http://xamarin.com/schemas/2014/forms"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="jamesMont.View.TimesPage"
             Title="Choose Time">
    <ContentPage.Content>





        <ListView x:Name="listView2" BackgroundColor="#3897f0" SeparatorColor="White"  ItemSelected="OnItemSelected">
            <ListView.ItemTemplate>
                <DataTemplate>
                    <TextCell TextColor="White" Text="{Binding Time}"  />
                </DataTemplate>
            </ListView.ItemTemplate>
        </ListView>
    </ContentPage.Content>
</ContentPage>